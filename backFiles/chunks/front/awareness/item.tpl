<li class="[[+active]]">
    <div class="collapsible-header">
        <i class="fa fa-plus"></i>
        <h2>[[+title]]
        </h2>
    </div>
    <div class="collapsible-body">
        <div class="row">
            <div class="col s12 m4 l4 awarenessImage" style="background-image: url('[[+image]]')">
            </div>
            <div class="col s12 m9 l8 awarenessParagraph">
                <p>
                    [[+description]]
                </p>
                [[+files]]
            </div>
        </div>
    </div>
</li>