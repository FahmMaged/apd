[[+head]]
[[+header]]

<section class="headerInner" style="background-image: url('[[+mainImage]]')">
    <img src="[[images/Events-DetailsBG.jpg]]">
    <div class="overlay">
        <div class="titles">
            <h3>[[+headerTitle]]
            </h3>
        </div>
    </div>
</section>

<section class="pressReleaseDetails">
    <div class="container">
        <div class="row">
            <h2>[[+title]]
            </h2>
            <h4>
                
                <i class="fa fa-calendar"></i>
                [[+publishDate]]


                <i class="fa fa-clock-o"></i>
                [[+end]] - [[+start]]

                <i class="fa fa-map-marker"></i>
                [[+location]]
            </h4>
            
            <div class="press-img">
            <img src="[[+image]]">
            </div>
            <h5>[[+details]] </h5>
            <p>
            	[[+description]]
            </p>

        </div>
    </div>
</section>

[[+footer]]