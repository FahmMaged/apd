<div class="col s12 m4 l4">
  <div class="newsImage">
    <div class="container-news">
      <div class="dateTime">
        <div>
          <h3>[[+day]]</h3>
          <h3>[[+monthName]]</h3>
        </div>
      </div>
      <img src="images/calendarEvents.png" class="calImg" />
    </div>
  </div>
  <a href="eventsDetails.php?id=[[+id]]&alias=[[+alias]]">
    <h3>[[+title]]</h3>
  </a>
  <p>[[+description]]</p>
  <h2>
    <div [[+hide]]>
      <i class="fa fa-clock-o"></i>
      [[+end]] - [[+start]]
    </div>
  </h2>
</div>
