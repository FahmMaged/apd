<div class="s12 l4 m4 col">
    <h3> [[+videos]]  </h3>
    <div class="s12 ">
            <iframe width="100%" height="180px" src="[[+link]]" frameborder="0"
            allow="autoplay; encrypted-media" allowfullscreen></iframe>
    </div>
    <h4>  [[+title]]                          </h4>
    <p>  
            [[+description]]
    </p> 

    <div class="watchBox">
        <div class="s6 col">
            <img src="images/team.png" />
        </div>
        <div class="s6 col">
            <h3 style="color:#710000" margin-bottom: 0;> [[+views]] </h3>
            <h3 style="margin-top: 0;     margin-top: -10px;"> [[+view]] </h3>
        </div>
    </div>
</div>