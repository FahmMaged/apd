[[+head]]
[[+header]]
<section class="headerInner" style="background-image: url('[[+mainImage]]')">
    <div class="overlay">
        <div class="titles">
            <h3>[[+headerTitle]]
            </h3>
        </div>
    </div>

</section>
    <!-- HEADER-END -->
<section class="pressReleaseDetails">
    <div class="container">
        <div class="row">
            <h2>[[+title]]
            </h2>
            <h4>
                    <i class="fa fa-calendar"></i>
                    [[+publishDate]]</h4>
            <img src="[[+image]]">
            <p>[[+description]]</p>

        </div>
    </div>
</section>
[[+footer]]